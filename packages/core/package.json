{
  "name": "@csereimarton/droplet",
  "version": "1.0.24",
  "description": "---",
  "keywords": [
    "droplet"
  ],
  "main": "build/droplet.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "rollup -c",
    "push": "npm run build && npm publish --access=public",
    "dev": "onchange \"src/*.js\" -- npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NuttyYokel/droplet.git"
  },
  "author": "Marton Cserei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NuttyYokel/droplet/issues"
  },
  "homepage": "https://github.com/NuttyYokel/droplet#readme",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "onchange": "^6.1.0",
    "rollup-plugin-babel": "^4.3.3"
  }
}
